<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="edward.audit.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Efectivo: 0.0" fx:id="label" style="-fx-font-size: 24px; -fx-font-weight: bold;"/>
    <Label text="" fx:id="warnings" style="-fx-font-size: 24px; -fx-font-weight: bold;"/>
    <Button text="Obtener" fx:id="button" alignment="CENTER" onAction="#onUpdateButton"/>
</VBox>
